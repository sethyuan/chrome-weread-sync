{
  "manifest_version": 3,
  "name": "Logseq微信读书同步助手",
  "version": "0.1.0",
  "description": "配合Logseq WeRead Sync插件使用，同步微信读书数据。",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/200.png"
  },
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    }
  },
  "options_ui": {
    "page": "src/options.html",
    "open_in_tab": false
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ]
}
